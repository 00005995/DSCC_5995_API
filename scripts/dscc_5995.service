[Unit]
Description=Example .NET Web API App running on Linux
[Service]
WorkingDirectory=/var/www/dscc_5995
ExecStart=/usr/bin/dotnet /var/www/dscc_5995/bin/Debug/netcoreapp3.0/DSCC_5995_API.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=dotnet
User=apache
[Install]
WantedBy=multi-user.target

